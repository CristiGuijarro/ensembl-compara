      <species_set>
        <taxonomic_group taxon_name="Testudines"/>
      </species_set>
    </one_vs_all>-->
    <all_vs_one method="LASTZ_NET" target_genome="ciona_savignyi">
      <species_set>
        <genome name="danio_rerio"/>
        <genome name="gallus_gallus"/>
        <genome name="ciona_intestinalis"/>
      </species_set>
    </all_vs_one>
    <!-- between refs -->
    <all_vs_all method="LASTZ_NET">
      <species_set>
        <genome name="danio_rerio"/>
        <genome name="petromyzon_marinus"/>
        <genome name="ciona_intestinalis"/>
        <genome name="eptatretus_burgeri"/>
      </species_set>
    </all_vs_all>
    <all_vs_all method="LASTZ_NET">
      <species_set>
        <genome name="mus_musculus"/>
        <genome name="gallus_gallus"/>
        <genome name="danio_rerio"/>
        <genome name="xenopus_tropicalis"/>
        <genome name="monodelphis_domestica"/>
        <genome name="latimeria_chalumnae"/>
      </species_set>
    </all_vs_all>
    <one_vs_all method="LASTZ_NET" ref_genome="mus_musculus">
      <species_set>
        <genome name="ornithorhynchus_anatinus"/>
        <genome name="bos_taurus"/>
        <genome name="canis_familiaris"/>
        <genome name="sus_scrofa"/>
        <genome name="ciona_intestinalis"/>
      </species_set>
    </one_vs_all>
    <pairwise_alignment method="LASTZ_NET" ref_genome="anolis_carolinensis" target_genome="pelodiscus_sinensis"/>
  </pairwise_alignments>
  <multiple_alignments>
    <multiple_alignment method="EPO">
      <species_set name="primates" in_collection="default">
        <taxonomic_group taxon_name="Primates" only_with_karyotype="1"/>
        <genome name="callithrix_jacchus"/><!--no karyotype stored in the core db, but the assembly has chromosomes-->
      </species_set>
    </multiple_alignment>
    <multiple_alignment method="EPO_LOW_COVERAGE">
      <species_set name="primates" in_collection="default">
        <taxonomic_group taxon_name="Primates"/>
      </species_set>
    </multiple_alignment>
    <multiple_alignment method="EPO">
      <species_set name="mammals" display_name="eutherian mammals" in_collection="default">
        <taxonomic_group taxon_name="Eutheria" only_with_karyotype="1"/>
        <genome name="callithrix_jacchus"/><!--no karyotype stored in the core db, but the assembly has chromosomes-->
      </species_set>
    </multiple_alignment>
    <multiple_alignment method="EPO_LOW_COVERAGE" gerp="1">
      <species_set name="mammals" display_name="eutherian mammals" in_collection="default">
        <taxonomic_group taxon_name="Eutheria"/>
      </species_set>
    </multiple_alignment>
    <multiple_alignment method="EPO">
      <species_set name="sauropsids" in_collection="default">
        <taxonomic_group taxon_name="Sauropsida" only_with_karyotype="1"/>
      </species_set>
    </multiple_alignment>
    <multiple_alignment method="EPO_LOW_COVERAGE" gerp="1">
      <species_set name="sauropsids" in_collection="default">
        <taxonomic_group taxon_name="Sauropsida"/>
      </species_set>
    </multiple_alignment>
    <multiple_alignment method="EPO">
      <species_set name="fish" in_collection="default">
        <taxonomic_group taxon_name="Actinopterygii" only_with_karyotype="1"/>
      </species_set>
    </multiple_alignment>
    <multiple_alignment method="EPO_LOW_COVERAGE" gerp="1">
      <species_set name="fish" in_collection="default">
        <taxonomic_group taxon_name="Actinopterygii"/>
      </species_set>
    </multiple_alignment>
    <multiple_alignment method="PECAN" gerp="1">
      <species_set name="amniotes" display_name="amniota vertebrates" in_collection="default">
        <taxonomic_group taxon_name="Amniota" only_with_karyotype="1"/>
        <genome name="callithrix_jacchus"/><!--no karyotype stored in the core db, but the assembly has chromosomes-->
      </species_set>
    </multiple_alignment>
    <multiple_alignment method="CACTUS_HAL">
      <species_set name="murinae" in_collection="murinae">
        <!--mus_caroli and mus_pahari are not in the set, so we can't take the whole collection-->
        <taxonomic_group taxon_name="Mus musculus"/>
        <genome name="mus_spretus"/>
        <genome name="rattus_norvegicus"/>
      </species_set>
    </multiple_alignment>
  </multiple_alignments>
  <self_alignments>
    <genome name="homo_sapiens"/>
  </self_alignments>
  <assembly_patches>
    <genome name="homo_sapiens"/>
    <genome name="mus_musculus"/>
  </assembly_patches>
  <families>
    <family collection="default"/>
  </families>
  <gene_trees>
    <protein_trees collection="default"/>
    <protein_trees collection="murinae"/>
    <nc_trees collection="default"/>
    <nc_trees collection="murinae"/>
  </gene_trees>
  <species_trees>
    <species_tree collection="ensembl"/>
  </species_trees>
</compara_db>
