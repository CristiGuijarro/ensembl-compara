      <species_set>
        <taxonomic_group taxon_name="Cyclostomata"/>
      </species_set>
    </one_vs_all>-->
    <!-- Empty at the moment -->
    <!--<one_vs_all method="LASTZ_NET" ref_genome="equus_caballus">
      <species_set>
        <taxonomic_group taxon_name="Perissodactyla"/>
      </species_set>
    </one_vs_all>-->
