<?xml version="1.0" encoding="UTF-8"?>
<compara_db division="ensembl">
  <collections>
    <collection name="default">
      <taxonomic_group taxon_name="Chordata">
        <ref_for_taxon name="mus_musculus"/>
      </taxonomic_group>
      <genome name="drosophila_melanogaster"/>
      <genome name="caenorhabditis_elegans"/>
      <genome name="saccharomyces_cerevisiae"/>
    </collection>
    <collection name="murinae">
      <taxonomic_group taxon_name="Murinae"/>
    </collection>
  </collections>
  <pairwise_alignments>
    <!-- reference genome vs whole clade -->
    <one_vs_all method="LASTZ_NET" ref_genome="homo_sapiens">
      <species_set in_collection="default">
        <taxonomic_group taxon_name="Chordata"/>
      </species_set>
    </one_vs_all>
    <one_vs_all method="LASTZ_NET" ref_genome="mus_musculus">
      <species_set in_collection="default">
        <taxonomic_group taxon_name="Rodentia"/>
      </species_set>
    </one_vs_all>
    <one_vs_all method="LASTZ_NET" ref_genome="gallus_gallus">
      <species_set>
        <taxonomic_group taxon_name="Sauropsida"/>
      </species_set>
    </one_vs_all>
    <one_vs_all method="LASTZ_NET" ref_genome="danio_rerio">
      <species_set>
        <!-- Include urochordates, cephalochordates, cyclostomes, cartilaginous fishes, ray-finned fishes, coelacanths and lungfishes -->
        <taxonomic_group taxon_name="Chordata"/>
        <taxonomic_group taxon_name="Tetrapoda" exclude="1"/>
      </species_set>
    </one_vs_all>
    <one_vs_all method="LASTZ_NET" ref_genome="oryzias_latipes">
      <species_set>
        <taxonomic_group taxon_name="Actinopterygii"/>
      </species_set>
    </one_vs_all>
    <one_vs_all method="LASTZ_NET" ref_genome="monodelphis_domestica">
      <species_set>
        <taxonomic_group taxon_name="Metatheria"/>
      </species_set>
    </one_vs_all>
    <one_vs_all method="LASTZ_NET" ref_genome="canis_familiaris">
      <species_set>
        <taxonomic_group taxon_name="Carnivora"/>
        <genome name="equus_caballus"/>
      </species_set>
    </one_vs_all>
    <one_vs_all method="LASTZ_NET" ref_genome="felis_catus">
      <species_set>
        <taxonomic_group taxon_name="Feliformia"/>
      </species_set>
    </one_vs_all>
    <!-- Included in an all-vs-one rule further down -->
    <!--<one_vs_all method="LASTZ_NET" ref_genome="ciona_intestinalis">
      <species_set>
        <taxonomic_group taxon_name="Ciona"/>
      </species_set>
    </one_vs_all>-->
    <!-- Empty at the moment -->
