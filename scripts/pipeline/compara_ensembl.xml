    <one_vs_all method="LASTZ_NET" ref_genome="bos_taurus">
      <species_set>
        <taxonomic_group taxon_name="Cetartiodactyla"/>
      </species_set>
    </one_vs_all>
    <pairwise_alignment method="LASTZ_NET" ref_genome="sus_scrofa" target_genome="ovis_aries"/>
    <!-- Equivalent to just one alignment at the moment -->
    <!--<one_vs_all method="LASTZ_NET" ref_genome="ovis_aries">
      <species_set>
        <taxonomic_group taxon_name="Caprinae"/>
      </species_set>
    </one_vs_all>-->
    <pairwise_alignment method="LASTZ_NET" ref_genome="ovis_aries" target_genome="capra_hircus"/>
    <!-- Empty at the moment -->
    <one_vs_all method="LASTZ_NET" ref_genome="latimeria_chalumnae">
      <species_set>
        <!--Coelacanths and lungfishes -->
        <taxonomic_group taxon_name="Sarcopterygii"/>
        <taxonomic_group taxon_name="Tetrapoda" exclude="1"/>
      </species_set>
    </one_vs_all>
    <!-- Empty at the moment -->
    <!--<one_vs_all method="LASTZ_NET" ref_genome="ornithorhynchus_anatinus">
      <species_set>
        <taxonomic_group taxon_name="Prototheria"/>
      </species_set>
    </one_vs_all>-->
