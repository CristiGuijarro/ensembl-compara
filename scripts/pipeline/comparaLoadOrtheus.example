>cat compara_hive_conf_file
{       HIVE_NAME  => "ortheus",
        INNODB_TABLES  => ["synteny_region", "dna", "seq_region"],
        SET_INTERNAL_IDS => {
                table_name => method_link_species_set_id,
        },
        NO_SET_DNA_TABLES => 0,

}

#HIVE_NAME : the name lsf will call your jobs
#INNODB_TABLES : change these tables to innodb
#SET_INTERNAL_IDS : for this hash of table names set them the primary key field to start at (method_link_species_set_id * 10000000000 + 1);
#The primary keys in the tables ("genomic_align", "genomic_align_block", "genomic_align_group", "genomic_align_tree") are automatically set to
#(ortheus_mlssid * 10000000000 + 1) unless the NO_SET_DNA_TABLES is set to 1
